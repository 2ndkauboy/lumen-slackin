{
    "name": "vluzrmos/lumen-slackin",
    "description": "Lumen Slack Invitator.",
    "keywords": ["Slack", "lumen", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "vluzrmos/slack-api": "0.0.*",
        "predis/predis": "~1.0",
        "illuminate/redis": "~5.0",
        "vluzrmos/lumen-socketio":"@dev",
        "tedivm/jshrink": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Mariuzzo\\LaravelJsLocalization\\": "workbench/vluzrmos/Laravel-JS-Localization/src/Mariuzzo/LaravelJsLocalization/"
        },
        "classmap": [
            "database/"
        ],
        "files":[
          "compat_l5.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },

    "minimum-stability": "stable"
}
