{
    "name": "vluzrmos/lumen-slackin",
    "description": "Lumen Slack Invitator.",
    "keywords": ["Slack", "lumen", "laravel"],
    "license": "DBAD",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "vluzrmos/slack-api": "~0.0",
        "vluzrmos/badge-poser": "~1.0",
        "predis/predis": "~1.0",
        "illuminate/redis": "~5.0",
        "badges/poser": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files":[
          "compat_l5.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },

    "minimum-stability": "stable"
}
