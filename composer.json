{
    "name": "vluzrmos/lumen-slackin",
    "description": "Lumen Slack Invitator.",
    "keywords": ["Slack", "lumen", "laravel"],
    "license": "DBAD",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "vluzrmos/slack-api": "0.0.*",
        "predis/predis": "~1.0",
        "illuminate/redis": "~5.0",
        "vluzrmos/lumen-socketio":"~0.1",
        "mariuzzo/laravel-js-localization":"dev-master",
        "badges/poser": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files":[
          "compat_l5.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:vluzrmos/Laravel-JS-Localization.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:vluzrmos/image.git"
        }
    ],

    "config": {
        "preferred-install": "dist"
    },

    "minimum-stability": "stable"
}
